void calcNeed(int need[3][3],int max[3][3],int alloc[3][3])
{
    for (int i=0;i<3;i++)
    {
        for (int j=0;j<3;j++)
 		{
        need[i][j]=max[i][j]-alloc[i][j];
    	}
	}
}
